<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - The_19_Module</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/normalize.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/The_19_Module.styles.css" asp-append-version="true" />
</head>
<body>

    <header class="_header">
        <ul class="header_list">
            <li class="header_list-item">
                <a href="">Главная</a>
            </li>
            <li class="header_list-item">
                <a href="">Помощь</a>
            </li>
            <li class="header_list-item">
                <a href="">Контакты</a>
            </li>
            <li class="header_list-item">
                <a href="">О нас</a>
            </li>
            <li class="header_list-item">
                <a href="">Вход</a>
            </li>
        </ul>
    </header>

    <div class="container">
        <div class="body_table_text">
            <img class="phone_img" src="~/images/phone.png">
            <h3 class="body_table_h1">Телефонная книга</h3>
        </div>
        <div class="body_table">
            <table class="main_table">
                <tr>
                    <th class="main_table_th">Id</th>
                    <th class="main_table_th">Фамилия</th>
                    <th class="main_table_th">Имя</th>
                    <th class="main_table_th">Отчество</th>
                </tr>
                <tr>
                    <td class="main_table_data">
                        <a href="">Id</a>
                    </td>
                    <td class="main_table_data">SecondName</td>
                    <td class="main_table_data">Name</td>
                    <td class="main_table_data">Otchestvo</td>
                </tr>
            </table>
        </div>
    </div>

    <main role="main" class="pb-3">
        @RenderBody()
    </main>

</body>
</html>

<div class="container">
    
</div>